{
    "version": "",
    "description": "The Outline Manager application creates and manages Outline servers, powered by Shadowsocks",
    "homepage": "https://github.com/Jigsaw-Code/outline-server",
    "license": "Apache-2.0",
    "notes": "We don't persist your Evernote data, they are still storaged in \"$env:APPDATA\\Outline Manager\".",
    "url": "",
    "hash": "",
    "installer": {
        "script": [
            "Expand-7zipArchive \"$dir\\`$PLUGINSDIR\\app-32.7z\" $dir -Removal",
            "Remove-Item \"$dir\\Uninstall Outline Manager.exe\"",
            "Remove-Item \"$dir\\`$PLUGINSDIR\" -Force -Recurse"
        ]
    },
    "shortcuts": [["Outline Manager.exe", "Outline Manager"]],
    "checkver": {
        "url": "https://github.com/Jigsaw-Code/outline-server/releases",
        "regex": "/releases/tag/(?:server-)([\\d-]+)"
    },
    "autoupdate": {
        "url": "https://github.com/Jigsaw-Code/outline-server/releases/download/server-$version/Outline-Manager.exe#/dl.zip"
    }
}
