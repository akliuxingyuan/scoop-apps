{
    "version": "1.1",
    "description": "RegJump. Open regedit with given path.",
    "homepage": "https://docs.microsoft.com/en-us/sysinternals/downloads/regjump",
    "license": {
        "identifier": "Freeware",
        "url": "https://docs.microsoft.com/en-us/sysinternals/license-terms"
    },
    "url": "https://download.sysinternals.com/files/RegJump.zip",
    "hash": "53dbc149d0bc0bf464a9970a14a1650fe0d2ca311dd80829c09589d0fe150eec",
    "pre_install": [
        "$sysint = 'HKCU:\\Software\\Sysinternals'",
        "$fin = \"$sysint\\Regjump\"",
        "if (-not (Test-Path $sysint)) { New-Item $sysint -Force | Out-Null }",
        "if (-not (Test-Path $fin)) { New-Item $fin -Force | Out-Null }",
        "Set-ItemProperty -Path $fin -Name 'EulaAccepted' -Value 1 -Type DWord -Force | Out-Null"
    ],
    "bin": "regjump.exe",
    "checkver": "RegJump\\s+v([\\d.]+)</h1",
    "autoupdate": {
        "url": "https://download.sysinternals.com/files/RegJump.zip"
    }
}
