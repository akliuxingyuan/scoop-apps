    "hash": "dd04b6e7d467b78c994fbb1929a9e7438c2e900ced4fcff245227c8fe9a237bd",
    "pre_install": [
        "$sysint = 'HKCU:\\Software\\Sysinternals'",
        "$fin = \"$sysint\\Disk2Vhd\"",
        "if (-not (Test-Path $sysint)) { New-Item $sysint -Force | Out-Null }",
        "if (-not (Test-Path $fin)) { New-Item $fin -Force | Out-Null }",
        "Set-ItemProperty -Path $fin -Name 'EulaAccepted' -Value 1 -Type DWord -Force | Out-Null"
    ],
    "bin": "disk2vhd.exe",
    "shortcuts": [
        [
            "disk2vhd.exe",
            "SysInternals\\Disk2vhd"
        ]
    ],
    "checkver": "Disk2vhd\\s+v([\\d.]+)</h1",
    "autoupdate": {
        "url": "https://download.sysinternals.com/files/Disk2vhd.zip"
    }
