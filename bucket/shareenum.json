{
    "version": "1.6",
    "description": "ShareEnum. Scan all the computers within the domains accessible to it, showing file and print shares and their security settings.",
    "homepage": "https://docs.microsoft.com/en-us/sysinternals/downloads/shareenum",
    "license": {
        "identifier": "Freeware",
        "url": "https://docs.microsoft.com/en-us/sysinternals/license-terms"
    },
    "url": "https://download.sysinternals.com/files/ShareEnum.zip",
    "hash": "54242bb3ce5f62c6847aa88ac3079ca58a1d508858ae16265e8e45a7b4ed1d38",
    "pre_install": [
        "$sysint = 'HKCU:\\Software\\Sysinternals'",
        "$fin = \"$sysint\\Share Enum\"",
        "if (-not (Test-Path $sysint)) { New-Item $sysint -Force | Out-Null }",
        "if (-not (Test-Path $fin)) { New-Item $fin -Force | Out-Null }",
        "Set-ItemProperty -Path $fin -Name 'EulaAccepted' -Value 1 -Type DWord -Force | Out-Null"
    ],
    "shortcuts": [
        [
            "ShareEnum.exe",
            "SysInternals\\ShareEnum"
        ]
    ],
    "checkver": "ShareEnum\\s+v([\\d.]+)</h1",
    "autoupdate": {
        "url": "https://download.sysinternals.com/files/ShareEnum.zip"
    }
}
