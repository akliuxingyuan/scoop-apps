{
    "version": "13.98",
    "description": "Autoruns. Autostart program viewer.",
    "homepage": "https://docs.microsoft.com/en-us/sysinternals/downloads/autoruns",
    "license": {
        "identifier": "Freeware",
        "url": "https://docs.microsoft.com/en-us/sysinternals/license-terms"
    },
    "url": "https://download.sysinternals.com/files/Autoruns.zip",
    "hash": "ec1836d93799c966365bcdd8dba474aef948d3b84cfd0477d0c3f862a32d822f",
    "installer": {
        "script": [
            "$sysint = 'HKCU:\\Software\\Sysinternals'",
            "$fin = \"$sysint\\AutoRuns\"",
            "New-Item $sysint, $fin -ErrorAction SilentlyContinue -Force | Out-Null",
            "Set-ItemProperty -Path $fin -Name 'EulaAccepted' -Value 1 -Type DWord -Force | Out-Null"
        ]
    },
    "uninstaller": {
        "script": [
            "if ($purge) {",
            "    $sysInt = 'HKCU:\\Software\\Sysinternals'",
            "    Remove-Item \"$sysInt\\AutoRuns\" -Force -Recurse",
            "    if ((Get-ChildItem $sysInt).Count -eq 0) { Remove-Item $sysInt -Force -Recurse }",
            "}"
        ]
    },
    "architecture": {
        "64bit": {
            "bin": [
                "autorunsc64.exe",
                [
                    "autorunsc64.exe",
                    "autoruns64"
                ],
                [
                    "autorunsc64.exe",
                    "autoruns"
                ]
            ],
            "shortcuts": [
                [
                    "Autoruns64.exe",
                    "SysInternals\\Autoruns"
                ]
            ]
        },
        "32bit": {
            "bin": [
                "autorunsc.exe",
                [
                    "autorunsc.exe",
                    "autoruns"
                ]
            ],
            "shortcuts": [
                [
                    "Autoruns.exe",
                    "SysInternals\\Autoruns"
                ]
            ]
        }
    },
    "checkver": "Autoruns for Windows\\s+v([\\d.]+)</h1",
    "autoupdate": {
        "url": "https://download.sysinternals.com/files/Autoruns.zip"
    }
}
