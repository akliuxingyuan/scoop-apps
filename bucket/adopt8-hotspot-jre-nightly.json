        "32bit": {
            "url": "https://github.com/AdoptOpenJDK/openjdk8-binaries/releases/download/jdk8u-2020-01-11-15-51/OpenJDK8U-jre_x86-32_windows_hotspot_2020-01-11-15-51.zip",
