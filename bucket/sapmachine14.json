    "checkver": {
        "url": "https://api.github.com/repos/SAP/SapMachine/releases",
        "re": "(?<sap>/sapmachine-(?:14[\\d.]*)(?:\\%2B|\\+)?(?:[\\d]*)/sapmachine-jdk-(?<version>[\\d.]+)(?:(?<dash>-)(?<ea>ea)\\.(?<build>[\\d]+))?_windows-x64_bin).zip",
