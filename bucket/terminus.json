    "shortcuts": [
        [
            "Terminus.exe",
            "Terminus"
        ]
    ],
    "checkver": {
        "github": "https://github.com/Eugeny/terminus"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://github.com/Eugeny/terminus/releases/download/v$version/terminus-$version-setup.exe#/dl.7z",
                "hash": {
                    "url": "$baseurl/latest.yml",
                    "regex": "sha512: $base64"
                }
            }
        }
    }
