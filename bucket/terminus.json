    "description": "A terminal for a more modern age.",
    "version": "1.0.98",
    "license": "MIT",
    "architecture": {
        "64bit": {
            "url": "https://github.com/Eugeny/terminus/releases/download/v1.0.98/terminus-1.0.98-setup.exe#/dl.7z",
            "hash": "sha512:d58a9fa28a59eff61a9f84cb60bff76fb42caf713df81b898d6765044cc173290fb5e23ca03f826312c40c25e1ffeca124b4a1c92bfc348f201dced01d4500d5",
            "installer": {
                "script": [
                    "Expand-7zipArchive \"$dir\\`$PLUGINSDIR\\app-64.7z\" \"$dir\"",
                    "Remove-Item \"$dir\\`$PLUGINSDIR\", \"$dir\\Unins*\" -Force -Recurse"
                ]
            },
            "bin": "Terminus.exe",
            "shortcuts": [
                [
                    "Terminus.exe",
                    "Terminus"
                ]
            ]
        }
    },
    "checkver": {
        "github": "https://github.com/Eugeny/terminus"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://github.com/Eugeny/terminus/releases/download/v$version/terminus-$version-setup.exe#/dl.7z",
                "hash": {
                    "url": "$baseurl/latest.yml",
                    "regex": "sha512: $base64"
                }
            }
        }
    }
}
