    "hash": "f69055772fcdd1e69d8aaefbb9a5234742e510449b8d2d01a74f39745095dada",
    "extract_dir": "tools",
    "bin": "GitVersion.exe",
    "checkver": {
        "url": "https://chocolatey.org/packages/GitVersion.Portable",
        "regex": "GitVersion ([\\w.-]+)"
    },
    "autoupdate": {
        "url": "https://packages.chocolatey.org/GitVersion.Portable.$version.nupkg",
