{
    "homepage": "https://www.pgadmin.org",
    "description": "pgAdmin is the most popular and feature rich Open Source administration and development platform for PostgreSQL. (Collected from [oltolm/scoop-nonportable](https://github.com/oltolm/scoop-nonportable))",
    "version": "4.26",
    "url": "https://ftp.postgresql.org/pub/pgadmin/pgadmin4/v4.26/windows/pgadmin4-4.26-x86.exe",
    "hash": "e377fc80d1ada7e40ea56411df2e209705025fe76f54840f81ce3a76eaa15f0c",
    "bin": [
        "runtime\\psql.exe",
        "runtime\\pgAdmin4.exe",
        "runtime\\pg_restore.exe",
        "runtime\\pg_dumpall.exe",
        "runtime\\pg_dump.exe"
    ],
    "installer": {
        "args": [
            "/verysilent",
            "/nocancel",
            "/norestart",
            "/dir=$dir"
        ]
    },
    "uninstaller": {
        "file": "unins000.exe",
        "args": [
            "/verysilent",
            "/nocancel",
            "/norestart"
        ]
    },
    "checkver": {
        "url": "https://www.pgadmin.org/download/pgadmin-4-windows/",
        "re": "pgAdmin 4 v([\\d.]+)"
    },
    "autoupdate": {
        "url": "https://ftp.postgresql.org/pub/pgadmin/pgadmin4/v$version/windows/pgadmin4-$version-x86.exe"
    }
}
