    "version": "14",
    "license": "GPL-2.0-only WITH Classpath-exception-2.0",
    "architecture": {
        "64bit": {
            "url": "https://github.com/SAP/SapMachine/releases/download//sapmachine-14/sapmachine-jre-14_windows-x64_bin.zip",
            "hash": "be5090c02588594542a007c23f552803a6dd9eee2ba330e34da8b243fe0b0414"
        }
    },
    "extract_dir": "sapmachine-jre-14",
    "env_add_path": "bin",
    "env_set": {
