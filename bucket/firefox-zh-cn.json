    "architecture": {
        "64bit": {
            "url": "https://download.mozilla.org/?product=firefox-70.0.1-SSL&os=win64&lang=zh-CN#/dl.7z",
            "hash": "1c36cf0001e2d08b99a4f45b1273087ef5bdafcf129df50c57d71eb037a5f283"
        },
        "32bit": {
            "url": "https://download.mozilla.org/?product=firefox-70.0.1-SSL&os=win&lang=zh-CN#/dl.7z",
