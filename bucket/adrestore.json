{
    "version": "1.1",
    "description": "AdRestore. Restore deleted ('tombstoned') objects.",
    "homepage": "https://docs.microsoft.com/en-us/sysinternals/downloads/adrestore",
    "license": {
        "identifier": "Freeware",
        "url": "https://docs.microsoft.com/en-us/sysinternals/license-terms"
    },
    "url": "https://download.sysinternals.com/files/ADRestore.zip",
    "hash": "68033fa10d80c9dfbea1d47967d6642e08488d661993e21961e27dac9adb875b",
    "pre_install": [
        "$sysint = 'HKCU:\\Software\\Sysinternals'",
        "$fin = \"$sysint\\A\"",
        "if (-not (Test-Path $sysint)) { New-Item $sysint -Force | Out-Null }",
        "if (-not (Test-Path $fin)) { New-Item $fin -Force | Out-Null }",
        "Set-ItemProperty -Path $fin -Name 'EulaAccepted' -Value 1 -Type DWord -Force | Out-Null"
    ],
    "bin": "adrestore.exe",
    "checkver": "AdRestore\\s+v([\\d.]+)</h1",
    "autoupdate": {
        "url": "https://download.sysinternals.com/files/ADRestore.zip"
    }
}
