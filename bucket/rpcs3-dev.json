    "version": "0.0.8-9652",
    "url": "https://github.com/rpcs3/rpcs3-binaries-win/releases/download/build-e005581ddae1fdf690f51e48d070f54281a47c1f/rpcs3-v0.0.8-9652-e005581d_win64.7z",
