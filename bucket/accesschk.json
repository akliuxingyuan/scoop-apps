{
    "version": "6.12",
    "description": "AccessChk. Reports effective permissions for securable objects.",
    "homepage": "https://docs.microsoft.com/en-us/sysinternals/downloads/accesschk",
    "license": {
        "identifier": "Freeware",
        "url": "https://docs.microsoft.com/en-us/sysinternals/license-terms"
    },
    "url": "https://download.sysinternals.com/files/AccessChk.zip",
    "hash": "84f9e8ed3dba19686ab536f0a9644a6c94f40ae92b172a5a1c48e3f78e48a284",
    "pre_install": [
        "$sysint = 'HKCU:\\Software\\Sysinternals'",
        "$fin = \"$sysint\\AccessChk\"",
        "if (-not (Test-Path $sysint)) { New-Item $sysint -Force | Out-Null }",
        "if (-not (Test-Path $fin)) { New-Item $fin -Force | Out-Null }",
        "Set-ItemProperty -Path $fin -Name 'EulaAccepted' -Value 1 -Type DWord -Force | Out-Null"
    ],
    "architecture": {
        "64bit": {
            "bin": [
                "accesschk64.exe",
                [
                    "accesschk64.exe",
                    "accesschk"
                ]
            ]
        },
        "32bit": {
            "bin": "accesschk.exe"
        }
    },
    "checkver": "AccessChk\\s+v([\\d.]+)</h1",
    "autoupdate": {
        "url": "https://download.sysinternals.com/files/AccessChk.zip"
    }
}
