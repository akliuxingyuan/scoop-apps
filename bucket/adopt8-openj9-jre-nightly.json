        "32bit": {
            "url": "https://github.com/AdoptOpenJDK/openjdk8-binaries/releases/download/jdk8u-2019-12-22-16-30/OpenJDK8U-jre_x86-32_windows_openj9_2019-12-22-16-30.zip",
