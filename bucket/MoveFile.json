{
    "version": "1.01",
    "description": "MoveFile. Copies over an in-use file at boot time.",
    "homepage": "https://docs.microsoft.com/en-us/sysinternals/downloads/movefile",
    "license": {
        "identifier": "Freeware",
        "url": "https://docs.microsoft.com/en-us/sysinternals/license-terms"
    },
    "url": "https://download.sysinternals.com/files/PendMoves.zip",
    "hash": "527143fc701ff297e57419d5200df82edb95b1dd564adaa50d40c5c93f6b36f6",
    "pre_install": [
        "$sysint = 'HKCU:\\Software\\Sysinternals'",
        "$fin = \"$sysint\\Movefile\"",
        "if (-not (Test-Path $sysint)) { New-Item $sysint -Force | Out-Null }",
        "if (-not (Test-Path $fin)) { New-Item $fin -Force | Out-Null }",
        "Set-ItemProperty -Path $fin -Name 'EulaAccepted' -Value 1 -Type DWord -Force | Out-Null"
    ],
    "bin": "movefile.exe",
    "checkver": "PendMoves v(?:[\\d.]+) and MoveFile\\s+v([\\d.]+)</h1",
    "autoupdate": {
        "url": "https://download.sysinternals.com/files/PendMoves.zip"
    }
}
