{
    "description": "AdoptOpenJDK 14 with Eclipse OpenJ9 JVM with large heap",
    "homepage": "https://adoptopenjdk.net",
    "version": "14.0.2-12-0.21.0",
    "license": "GPL-2.0-only WITH Classpath-exception-2.0,EPL-2.0",
    "architecture": {
        "64bit": {
            "url": "https://github.com/AdoptOpenJDK/openjdk14-binaries/releases/download/jdk-14.0.2%2B12_openj9-0.21.0/OpenJDK14U-jdk_x64_windows_openj9_windowsXL_14.0.2_12_openj9-0.21.0.zip",
            "hash": "31ea28f290dacadd53d99ac7d53773b28cb832d09f4854062ad6bba56c6a68c6"
        }
    },
    "extract_dir": "jdk-14.0.2+12",
    "env_add_path": "bin",
    "env_set": {
        "JAVA_HOME": "$dir"
    },
    "checkver": {
        "url": "https://api.adoptopenjdk.net/v3/assets/feature_releases/14/ga?jvm_impl=openj9&heap_size=large&os=windows&architecture=x64&image_type=jdk&project=jdk&vendor=adoptopenjdk&page_size=1&sort_order=DESC",
        "jp": "$..binaries[0].package.link",
        "re": "https://github.com/(?<url>.*?(?<tag>jdk-(?<major>(?<jdk>[\\d]+)[\\d.]+)(?:\\%2B|\\+)(?<build>[\\d]+)(?<patch>[\\d.]*)(?:(?<oj>_openj9?)(?<jvmver>-[\\d.]+))?)/.*?).zip",
        "replace": "${major}-${build}${patch}${jvmver}"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://github.com/$matchUrl.zip"
            }
        },
        "hash": {
            "url": "$url.sha256.txt",
            "find": "^([a-fA-F0-9]+)\\s"
        },
        "extract_dir": "jdk-$matchMajor+$matchBuild"
    }
}
