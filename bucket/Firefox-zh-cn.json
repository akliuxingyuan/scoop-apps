            "hash": "983522bd1c0298f8dab039e93ef4007d665b2f60c02f0b06ad454d6faed9be94"
        },
        "32bit": {
            "url": "https://download.mozilla.org/?product=firefox-72.0.2-SSL&os=win&lang=zh-CN#/dl.7z",
