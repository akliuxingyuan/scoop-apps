{
    "version": "1.1",
    "description": "FindLinks. Reports the file index and locate any hard links.",
    "homepage": "https://docs.microsoft.com/en-us/sysinternals/downloads/findlinks",
    "license": {
        "identifier": "Freeware",
        "url": "https://docs.microsoft.com/en-us/sysinternals/license-terms"
    },
    "url": "https://download.sysinternals.com/files/FindLinks.zip",
    "hash": "a651cb5050544d187557e08df7ef11d94e3f6ae67a902264e18fe3701d7235a6",
    "pre_install": [
        "$sysint = 'HKCU:\\Software\\Sysinternals'",
        "$fin = \"$sysint\\FindLinks\"",
        "if (-not (Test-Path $sysint)) { New-Item $sysint -Force | Out-Null }",
        "if (-not (Test-Path $fin)) { New-Item $fin -Force | Out-Null }",
        "Set-ItemProperty -Path $fin -Name 'EulaAccepted' -Value 1 -Type DWord -Force | Out-Null"
    ],
    "architecture": {
        "64bit": {
            "bin": [
                "FindLinks64.exe",
                [
                    "FindLinks64.exe",
                    "FindLinks"
                ]
            ]
        },
        "32bit": {
            "bin": "FindLinks.exe"
        }
    },
    "checkver": "FindLinks\\s+v([\\d.]+)</h1",
    "autoupdate": {
        "url": "https://download.sysinternals.com/files/FindLinks.zip"
    }
}
