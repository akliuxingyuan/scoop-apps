    "hash": "940b55500af1e80ad7bb796e38639db3cafd81dd9087173f7e93e97012ba0248",
    "pre_install": [
        "$sysint = 'HKCU:\\Software\\Sysinternals'",
        "$fin = \"$sysint\\LiveKd\"",
        "if (-not (Test-Path $sysint)) { New-Item $sysint -Force | Out-Null }",
        "if (-not (Test-Path $fin)) { New-Item $fin -Force | Out-Null }",
        "Set-ItemProperty -Path $fin -Name 'EulaAccepted' -Value 1 -Type DWord -Force | Out-Null"
    ],
    "architecture": {
        "64bit": {
            "bin": [
                "livekd64.exe",
                [
                    "livekd64.exe",
                    "livekd"
                ]
            ]
        },
        "32bit": {
            "bin": "livekd.exe"
        }
    },
    "checkver": "LiveKd\\s+v([\\d.]+)</h1",
    "autoupdate": {
        "url": "https://download.sysinternals.com/files/LiveKD.zip"
    }
