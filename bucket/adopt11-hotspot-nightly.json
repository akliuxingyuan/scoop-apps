        },
        "32bit": {
            "url": "https://github.com/AdoptOpenJDK/openjdk11-binaries/releases/download/jdk11u-2020-03-18-11-36/OpenJDK11U-jdk_x86-32_windows_hotspot_2020-03-18-11-36.zip",
            "hash": "e033b55f95bef4cd3a8e83ef5917134ee070250badc9322bf4ffffb36358c1f1"
