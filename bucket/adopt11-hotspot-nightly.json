        },
        "32bit": {
            "url": "https://github.com/AdoptOpenJDK/openjdk11-binaries/releases/download/jdk11u-2020-03-28-08-27/OpenJDK11U-jdk_x86-32_windows_hotspot_2020-03-28-08-27.zip",
            "hash": "312d5ebdeb143b511058d2640550f1bfb31604f54d67bf5b4e332c3fa61973f7"
