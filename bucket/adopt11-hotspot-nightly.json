        },
        "32bit": {
            "url": "https://github.com/AdoptOpenJDK/openjdk11-binaries/releases/download/jdk11u-2020-02-23-13-13/OpenJDK11U-jdk_x86-32_windows_hotspot_2020-02-23-13-13.zip",
            "hash": "2f99794eb9f7ce662da03e78bcde99225f90cc3df65edacc7fbaec02389f0e4f"
