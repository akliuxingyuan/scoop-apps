            "hash": "bae1e466e464d5b765dfc82dacd87ab705cfd76438924c425dedd08779dfeaf8"
        },
        "32bit": {
            "url": "https://github.com/AdoptOpenJDK/openjdk11-binaries/releases/download/jdk11u-2019-12-07-05-08/OpenJDK11U-jre_x86-32_windows_hotspot_2019-12-07-05-08.zip",
            "hash": "6119442655db043dd0754008f81f45dc633efe19b85fd7c04772d82337b276ef"
        }
