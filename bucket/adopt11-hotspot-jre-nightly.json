        },
        "32bit": {
            "url": "https://github.com/AdoptOpenJDK/openjdk11-binaries/releases/download/jdk11u-2020-02-25-11-25/OpenJDK11U-jre_x86-32_windows_hotspot_2020-02-25-11-25.zip",
            "hash": "21567d08a6b3bfb6ef96c178225a7f27b426992d0847451c81eb8115ddc985fe"
        }
