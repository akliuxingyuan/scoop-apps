{
    "version": "1.4",
    "description": "LogonSessions. List the active logon sessions on a system.",
    "homepage": "https://docs.microsoft.com/en-us/sysinternals/downloads/logonsessions",
    "license": {
        "identifier": "Freeware",
        "url": "https://docs.microsoft.com/en-us/sysinternals/license-terms"
    },
    "url": "https://download.sysinternals.com/files/logonSessions.zip",
    "hash": "116e748ceba543371cce0d3dffb4e13b9a91ee27ee28575aa34eda97ba97cf9c",
    "pre_install": [
        "$sysint = 'HKCU:\\Software\\Sysinternals'",
        "$fin = \"$sysint\\LogonSessions\"",
        "if (-not (Test-Path $sysint)) { New-Item $sysint -Force | Out-Null }",
        "if (-not (Test-Path $fin)) { New-Item $fin -Force | Out-Null }",
        "Set-ItemProperty -Path $fin -Name 'EulaAccepted' -Value 1 -Type DWord -Force | Out-Null"
    ],
    "architecture": {
        "64bit": {
            "bin": [
                "logonsessions64.exe",
                [
                    "logonsessions64.exe",
                    "logonsessions"
                ]
            ]
        },
        "32bit": {
            "bin": "logonsessions.exe"
        }
    },
    "checkver": "LogonSessions\\s+v([\\d.]+)</h1",
    "autoupdate": {
        "url": "https://download.sysinternals.com/files/logonSessions.zip"
    }
}
