{
    "description": "Official production-ready open-source builds of OpenJDK 16",
    "homepage": "https://jdk.java.net/16",
    "version": "16-29-ea",
    "license": "GPL-2.0-only WITH Classpath-exception-2.0",
    "architecture": {
        "64bit": {
            "url": "https://download.java.net/java/early_access/jdk16/29/GPL/openjdk-16-ea+29_windows-x64_bin.zip",
            "hash": "c648613342ac89f1899f91185499b26ea19abc05bc6023a288bede03bb696543"
        }
    },
    "extract_dir": "jdk-16",
    "env_add_path": "bin",
    "env_set": {
        "JAVA_HOME": "$dir"
    },
    "checkver": {
        "re": "/(?<type>early_access|GA)/(?<path>jdk(?<major>[\\d.]+)(?:.*)?/(?<build>[\\d]+)(?:/GPL|/binaries)?)/(?<file>openjdk-(?<version>[\\d.]+)(?<ea>-ea)?(?:\\+[\\d]+)?_windows-x64_bin.(zip|tar.gz))",
        "replace": "${version}-${build}${ea}"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://download.java.net/java/$matchType/$matchPath/$matchFile"
            }
        },
        "hash": {
            "url": "$url.sha256"
        },
        "extract_dir": "jdk-$matchVersion"
    }
}
