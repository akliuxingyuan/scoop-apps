{
    "version": "4.18",
    "homepage": "https://www.pgadmin.org/",
    "description": "PostgreSQL administration and development platform.",
    "license": "PostgreSQL",
    "url": "https://ftp.postgresql.org/pub/pgadmin/pgadmin4/v4.18/windows/pgadmin4-4.18-x86.exe",
    "hash": "ac42960439a46463e4e43cba526d33bbdbe985ee756cf51982f677dd1d24a666",
    "installer": {
        "args": [
            "/verysilent",
            "/nocancel",
            "/norestart",
            "/dir=$dir"
        ]
    },
    "post_install": "Remove-Item -Recurse \"$env:ProgramData\\Microsoft\\Windows\\Start Menu\\Programs\\pgAdmin 4\"",
    "uninstaller": {
        "file": "unins000.exe",
        "args": [
            "/verysilent",
            "/nocancel",
            "/norestart"
        ]
    },
    "bin": [
        "runtime\\psql.exe",
        "runtime\\pgAdmin4.exe",
        "runtime\\pg_restore.exe",
        "runtime\\pg_dumpall.exe",
        "runtime\\pg_dump.exe"
    ],
    "shortcuts": [
        [
            "runtime\\pgAdmin4.exe",
            "pgAdmin 4"
        ]
    ],
    "checkver": {
        "url": "https://www.pgadmin.org/download/pgadmin-4-windows/",
        "regex": "pgAdmin 4 v([\\d.]+)"
    },
    "autoupdate": {
        "url": "https://ftp.postgresql.org/pub/pgadmin/pgadmin4/v$version/windows/pgadmin4-$version-x86.exe"
    }
}
