{
    "version": "1.02",
    "description": "PipeList. Displays the named pipes on system, including the number of maximum instances and active instances for each pipe.",
    "homepage": "https://docs.microsoft.com/en-us/sysinternals/downloads/pipelist",
    "license": {
        "identifier": "Freeware",
        "url": "https://docs.microsoft.com/en-us/sysinternals/license-terms"
    },
    "url": "https://download.sysinternals.com/files/PipeList.zip",
    "hash": "e9c8d86659cfa1a89890d92036ff8989c1be12dfdcfbe2a1f1961fe15f8bc306",
    "pre_install": [
        "$sysint = 'HKCU:\\Software\\Sysinternals'",
        "$fin = \"$sysint\\PipeList\"",
        "if (-not (Test-Path $sysint)) { New-Item $sysint -Force | Out-Null }",
        "if (-not (Test-Path $fin)) { New-Item $fin -Force | Out-Null }",
        "Set-ItemProperty -Path $fin -Name 'EulaAccepted' -Value 1 -Type DWord -Force | Out-Null"
    ],
    "architecture": {
        "64bit": {
            "bin": [
                "pipelist64.exe",
                [
                    "pipelist64.exe",
                    "pipelist"
                ]
            ]
        },
        "32bit": {
            "bin": "pipelist.exe"
        }
    },
    "checkver": "PipeList\\s+v([\\d.]+)</h1",
    "autoupdate": {
        "url": "https://download.sysinternals.com/files/PipeList.zip"
    }
}
