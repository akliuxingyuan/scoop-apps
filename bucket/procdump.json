{
    "version": "9.0",
    "description": "ProcDump. Capture process dumps of otherwise difficult to isolate and reproduce CPU spikes.",
    "homepage": "https://docs.microsoft.com/en-us/sysinternals/downloads/procdump",
    "license": {
        "identifier": "Freeware",
        "url": "https://docs.microsoft.com/en-us/sysinternals/license-terms"
    },
    "url": "https://download.sysinternals.com/files/Procdump.zip",
    "hash": "9e089b3f25a85c58f901094e4126412bbef480bd7baef2a9039bdef328bf5baa",
    "pre_install": [
        "$sysint = 'HKCU:\\Software\\Sysinternals'",
        "$fin = \"$sysint\\ProcDump\"",
        "if (-not (Test-Path $sysint)) { New-Item $sysint -Force | Out-Null }",
        "if (-not (Test-Path $fin)) { New-Item $fin -Force | Out-Null }",
        "Set-ItemProperty -Path $fin -Name 'EulaAccepted' -Value 1 -Type DWord -Force | Out-Null"
    ],
    "architecture": {
        "64bit": {
            "bin": [
                "procdump64.exe",
                [
                    "procdump64.exe",
                    "procdump"
                ]
            ]
        },
        "32bit": {
            "bin": "procdump.exe"
        }
    },
    "checkver": "ProcDump\\s+v([\\d.]+)</h1",
    "autoupdate": {
        "url": "https://download.sysinternals.com/files/Procdump.zip"
    }
}
