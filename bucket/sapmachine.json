    "checkver": {
        "url": "https://api.github.com/repos/SAP/SapMachine/releases",
        "re": "(?<sap>/sapmachine-(?:[\\d.]+)(?:\\%2B|\\+)?(?:[\\d]*)/sapmachine-jdk-(?<version>[\\d.]+)_windows-x64_bin).zip"
