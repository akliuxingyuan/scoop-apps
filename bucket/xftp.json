{
    "homepage": "https://www.netsarang.com/zh/Xftp/",
    "description": "业界最强大的SSH客户机",
    "version": "6.0.0191",
    "depends": [
        "bluelifehostseditor",
        "vcredist2008",
        "vcredist2012",
        "sudo"
    ],
    "url": "https://zapps.ziiyc.com/Xftp/Xftp-6.0.0191r.exe#/setup.exe",
    "hash": "9b0763884355e4c8820e406040ee451a1051732b67eedffad85b751fd9ce07fd",
    "pre_install": [
        "sudo hostseditor.exe /a transact.netsarang.com",
        "sudo hostseditor.exe /a update.netsarang.com",
        "sudo hostseditor.exe /a www.netsarang.com",
        "sudo hostseditor.exe /a www.netsarang.co.kr",
        "sudo hostseditor.exe /a sales.netsarang.com",
        "sudo hostseditor.exe /a active.netsarang.com",
        "sudo hostseditor.exe /a up.netsarang.com"
    ],
    "installer": {
        "script": [
            "Start-Process -Wait \"$dir\\setup.exe\" \"/b$dir /S /v/qn\" -Verb RunAs",
            "Start-Process -Wait msiexec \"/i `\"$dir\\Xftp` 6.msi`\" INSTALLDIR=`\"$dir`\" /qb\" -Verb RunAs"
        ],
        "keep": true
    },
    "post_install": [
        "$null, $bucket = find_manifest($app)",
        "$bucketdir = Find-BucketDirectory($bucket)",
        "$escdir = $dir -replace \"\\\\\",\"\\\\\"",
        "(gc \"$bucketdir\\..\\scripts\\xftp\\register.reg.templ\") | out-file -enc oem \"$dir\\register.reg\"",
        "sudo regedit /s $dir\\register.reg",
        "Remove-Item \"$env:PUBLIC\\Desktop\\Xmanager` Power` Suite` 6\" -Recurse",
        "sudo Remove-Item \"$env:ProgramData\\Microsoft\\Windows\\Start` Menu\\Programs\\SHELL_OBJECT_FOLDER\" -Recurse",
        "ie4uinit.exe -show"
    ],
    "uninstaller": {
        "script": "Start-Process -Wait msiexec \"/x `\"$dir\\Xftp` 6.msi`\" /qb\" -Verb RunAs"
    },
    "shortcuts": [
        [
            "Xftp.exe",
            "Xftp"
        ],
        [
            "Xagent.exe",
            "Xagent"
        ]
    ],
    "checkver": {
        "url": "https://zapps.ziiyc.com/Xftp/Xftp.latest",
        "regex": "([\\d.]+)"
    },
    "autoupdate": {
        "url": "https://zapps.ziiyc.com/Xftp/Xftp-$versionr.exe#/setup.exe",
        "hash": {
            "url": "$url.sha256",
            "regex": "$sha256"
        }
    }
}
