        },
        "32bit": {
            "url": "https://github.com/AdoptOpenJDK/openjdk8-binaries/releases/download/jdk8u-2020-08-01-03-04/OpenJDK8U-jdk_x86-32_windows_openj9_2020-08-01-03-04.zip",
            "hash": "b8206819274c8145d1c395edca58c6342730cdd715c80c26b64f5b26d1be5056"
