{
    "homepage": "https://www.iobit.com/en/iobit-unlocker.php",
    "version": "1.1.2",
    "url": "http://update.iobit.com/dl/unlocker-setup.exe#/setup.exe",
    "installer": {
        "script": [
            "Start-Process -Wait \"$dir\\setup.exe\" \"/VERYSILENT /DIR=$dir /NORESTART\" -Verb RunAs -PassThru -NoNewWindow"
        ]
    },
    "post_install": [
        "Get-WmiObject Win32_Process -Filter \"name='IObitUnlocker.exe'\" | Invoke-WmiMethod -Name Terminate | Out-Null",
        "Remove-Item \"$env:PUBLIC\\Desktop\\IObit` Unlocker.lnk\""
    ],
    "uninstaller": {
        "script": [
            "Start-Process -Wait \"$dir\\unins000.exe\" \"/VERYSILENT /NORESTART\" -Verb RunAs -PassThru -NoNewWindow"
        ]
    }
}
